{
  "compilerOptions": {
    "target": "ES2020",
    "module": "node16",
    "moduleResolution": "nodenext",
    
    "rootDir": "./src",
    "outDir": "./dist",

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src"]
}
